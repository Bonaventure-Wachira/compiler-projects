    $end (0) 0
    '(' (40) 27
    ')' (41) 27
    ':' (58) 5
    ';' (59) 2 9 10 11
    error (256) 11
    IDENTIFIER <iden> (258) 2 5 31
    INT_LITERAL <value> (259) 30
    REAL_LITERAL <value> (260) 28
    BOOLEAN_LITERAL <value> (261) 29
